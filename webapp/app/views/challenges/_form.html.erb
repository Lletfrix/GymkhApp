<%= simple_form_for(@challenge) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :description %>
    <div class="form-group">
      <select name="challenge[foursquare_id]" class="fsq-select form-control"></select>
    </div>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
